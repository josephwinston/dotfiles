# Use the C Shell

alias running "ps ax | grep \!* |& grep -v 'grep \!*' "
alias zap '(set j=`ps ax|grep \!*|head -1`;kill -9 $j[1]) >& /dev/null'

alias ls "ls -C --color=auto"

alias versions "rpm -q -a --queryformat '%{NAME} %{VERSION}\n'"

