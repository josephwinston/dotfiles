*customization:  -color

! Turn backing store on
*BackingStore:		Always
*SaveUnder: 		True

OpenWindows.WorkspaceColor:	#40a0c0
OpenWindows.WindowColor:	#cccccc
OpenWindows.IconLocation:	bottom
OpenWindows.DragRightDistance:	100
OpenWindows.SelectDisplaysMenu:	False
OpenWindows.SetInput:	followmouse
OpenWindows.Beep:	always
OpenWindows.ScrollbarPlacement:	right
OpenWindows.PopupJumpCursor:	True
OpenWindows.MultiClickTimeout:	4
Scrollbar.JumpCursor:	True
*basicLocale:	C
*displayLang:	C
*inputLang:	C
*numeric:	C
*timeFormat:	C

xbiff.Geometry:	48x48

xclock*Geometry:	90x90
#ifdef COLOR
xclock*Background:	skyblue
#endif
xclock*wm_option.title:	off

Clock*Background:     grey
Clock*BorderColor:    light blue
Clock*hour:           yellow
Clock*jewel:          yellow
Clock*minute:         yellow

xcutsel.Font:	9x15

!
! xemacs 18.54 and 19.XX
!
Emacs.Geometry:	90x42+35+65
Emacs.BitMapIcon:	on
Emacs.font:		*-courier-medium-r-*-*-*-120-*-*-*-*-*-*
!Emacs.font:		-*-fixed-medium-r-*-*-*-140*-*-*-*-*-*-*
!Emacs.italic.attributeFont:  -*-courier-medium-o-*-*-*-140-*-*-*-*-iso8859-*
!Emacs.bold-italic.attributeFont:  -*-courier-medium-o-*-*-*-140-*-*-*-*-iso8859-*

xload*Foreground:	black
xload*Geometry:	180x90
xload*Background:	white
xload*wm_option.title:	off

#ifdef COLOR
!xman*Command.foreground:	Blue
!xman*Text.border:	Red
!xman*topBox*foreground:	Blue
#endif

XTerm*saveLines:	300
XTerm*waitforwm:	false
xTerm*Font: 		-*-fixed-medium-r-*-*-*-120*-*-*-*-*-*-*
XTerm*Font: 		-*-fixed-medium-r-*-*-*-120*-*-*-*-*-*-*
XTerm*background:	white
XTerm*foreground:	black
XTerm*scrollBar:	false
XTerm*cursorColor:	black

